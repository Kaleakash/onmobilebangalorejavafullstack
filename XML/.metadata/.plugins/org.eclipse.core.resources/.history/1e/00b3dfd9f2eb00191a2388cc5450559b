	<xsd:complexType>
	<xsd:attribute name="FName" type="xsd:string"></xsd:attribute>
	<xsd:attribute name="LName" type="xsd:string"></xsd:attribute>
	</xsd:complexType>
</xsd:element>	
<xsd:element name="Salary" type="xsd:double"></xsd:element>			
			</xsd:sequence>
		</xsd:complexType>
		</xsd:element>	
		</xsd:sequence>
	</xsd:complexType>
</xsd:element>

